{
  "name": "summit_page",
  "version": "2018.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "autoprefixer": "8.6.4",
    "babel-loader": "8.0.0-beta.4",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "cssnano": "3.10.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "1.1.11",
    "gh-pages": "1.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0-rc.9",
    "imagemin-webpack-plugin": "2.1.5",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "optimize-css-assets-webpack-plugin": "4.0.3",
    "postcss-loader": "2.1.5",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "svg-url-loader": "2.3.2",
    "url-loader": "1.0.1",
    "webpack": "4.13.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "normalize-scss": "7.0.1",
    "smooth-scroll": "github:cferdinandi/smooth-scroll#8a210c669ef3361d59ac150f521c14f9a624a78e"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "prettier:scss:list": "prettier --config .prettierrc --parser scss -l src/**/*.scss || true",
    "prettier:js:list": "prettier --config .prettierrc --parser babylon -l src/**/*.js || true",
    "prettier:scss:write": "prettier --config .prettierrc --parser scss --write src/**/*.scss",
    "prettier:js:write": "prettier --config .prettierrc --parser babylon --write src/**/*.js",
    "prettier:write": "npm run prettier:scss:write && npm run prettier:js:write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
